<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:deviceGroups="[mobile/groups/responsive]"
            cq:lastModified="{Date}2025-05-14T21:05:16.444+05:30"
            cq:lastModifiedBy="admin"
            cq:template="/conf/bt-sample-project/settings/wcm/templates/demo-editable-template"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="bt-sample-project/components/page">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="bt-sample-project/components/container"
                layout="responsiveGrid">
            <experiencefragment-header
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bt-sample-project/components/experiencefragment"
                    fragmentVariationPath="/content/experience-fragments/bt-sample-project/us/en/site/header/master"/>
            <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bt-sample-project/components/container"
                    editable="{Boolean}true"
                    layout="responsiveGrid"/>
            <carousel
                    jcr:created="{Date}2025-05-13T21:20:20.331+05:30"
                    jcr:createdBy="admin"
                    jcr:lastModified="{Date}2025-05-13T21:20:20.331+05:30"
                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="bt-sample-project/components/carousel">
                <cq:responsive jcr:primaryType="nt:unstructured">
                    <default
                            jcr:primaryType="nt:unstructured"
                            offset="0"
                            width="8"/>
                </cq:responsive>
            </carousel>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                        jcr:primaryType="nt:unstructured"
                        title="Smaller Screen"
                        width="{Long}768"/>
                <tablet
                        jcr:primaryType="nt:unstructured"
                        title="Tablet"
                        width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
